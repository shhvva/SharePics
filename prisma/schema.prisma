generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  image   String
  caption String
  userId    String
  userName String
  userPhoto String
  createdAt DateTime @default(now())
}
